{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowCloudFrontOACOnly",
      "Effect": "Allow",
      "Principal": { "Service": "cloudfront.amazonaws.com" },
      "Action": "s3:GetObject",
      "Resource": "ARN",
      "Condition": {
        "StringEquals": {
          "AWS:SourceArn": "CLOUDFRONT ARN"
        }
      }
    }
  ]
}

